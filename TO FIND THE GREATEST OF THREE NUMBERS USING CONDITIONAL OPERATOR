//INPUT
import java.util.*;
public class conditional_operator{
  public static void main(String[] args){
    int a=10,b=20,c=5;
    int r =(a>b)?(a>c?a:c):(b>c?b:c);
    System.out.println("The Greatest of three numbers is:"+r);
  } 
}
//CONDITIONAL OPERATOR
//OUTPUT
The Greatest of three numbers is:20
